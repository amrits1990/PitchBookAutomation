# Environment Configuration for SECFinancialRAG

# Database Configuration (REQUIRED)
# PostgreSQL connection details
DB_HOST=localhost
DB_PORT=5432
DB_NAME=financial_data
DB_USER=postgres
DB_PASSWORD=your_password_here

# SEC API Configuration (REQUIRED)
SEC_API_USER_AGENT=YourCompany contact@youremail.com

# Processing Configuration
MAX_RETRIES=3
RETRY_DELAY=1
BATCH_SIZE=100

# LTM Export Configuration
LTM_EXPORT_DIR=./ltm_exports

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=sec_financial_rag.log

# Optional: Rate limiting
REQUEST_DELAY=0.1
