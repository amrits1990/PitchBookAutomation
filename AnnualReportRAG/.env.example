# Environment Configuration for AnnualReportRAG

# SEC API Configuration (REQUIRED)
# You must provide a valid email address for SEC API access
# This is required by the SEC EDGAR API terms of service
SEC_USER_AGENT=your.email@example.com

# Rate Limiting Configuration
RATE_LIMIT_REQUESTS=10

# Processing Configuration
MAX_FILES_PER_TICKER=150
DEFAULT_CHUNK_SIZE=800
DEFAULT_OVERLAP=150

# Output Configuration
OUTPUT_BASE_DIR=./data
CLEANUP_AFTER_PROCESSING=false

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=annual_report_rag.log
